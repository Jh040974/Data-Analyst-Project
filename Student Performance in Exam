    "        y=df[feature],\n",
    "        hue=df[feature],  # Assign hue to match y\n",
    "        order=df[feature].value_counts().index,\n",
    "        palette=palettes[i % len(palettes)],\n",
    "        legend=False  # Suppress redundant legend\n",
    "    )\n",
